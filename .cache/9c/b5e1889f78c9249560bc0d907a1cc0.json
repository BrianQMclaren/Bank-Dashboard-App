iddle\")\n      .attr(\"class\", \"percentage-complete\")\n      .attr(\"dy\", \".3em\");\n\n    d3.transition()\n      .duration(1000)\n      .tween(\"progress\", () => {\n        return t => {\n          progress = i(t);\n          percentage.text(formatPercent(progress));\n        };\n      });\n\n    arcs\n      .append(\"path\")\n      .attr(\"fill\", (d, i) => {\n        return color(i);\n      })\n      .attr(\"d\", arc);\n  }\n\n  render() {\n    return <svg width={this.width} height={this.height} />;\n  }\n}\n\nexport default PieChart;\n"},"lineCount":null}},"error":null,"hash":"8ae44102b5a2a2ce782539acb2eb070c","cacheData":{"env":{}}}